[Unit]
Description=
After=network.target
StartLimitIntervalSec=60

[Service]
Type=simple
Restart=always
RestartSec=60
WorkingDirectory=
ExecStart=
ExecStop=
KillMode=control-group
KillSignal=SIGTERM
